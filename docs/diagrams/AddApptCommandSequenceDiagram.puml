@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":CommandParser" as AddApptCommandParser LOGIC_COLOR
participant "c:AddApptCommand" as AddApptCommand LOGIC_COLOR
participant "appt:Appointment" as Appointment LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
participant "target:Person" as Person LOGIC_COLOR
participant "edited:Person" as Person LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("addAppt i/S1234567A @d/2023-12-25 @t/1400-1500")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("addAppt i/S1234567A @d/2023-12-25 @t/1400-1500")
activate AddressBookParser

create AddApptCommandParser
AddressBookParser -> AddApptCommandParser
activate AddApptCommandParser

AddApptCommandParser --> AddressBookParser
deactivate AddApptCommandParser

AddressBookParser -> AddApptCommandParser : parse("i/S1234567A @d/2023-12-25 @t/1400-1500")
activate AddApptCommandParser

create AddApptCommand
AddApptCommandParser -> AddApptCommand
activate AddApptCommand

AddApptCommand --> AddApptCommandParser
deactivate AddApptCommand

AddApptCommandParser --> AddressBookParser : c
deactivate AddApptCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AddApptCommandParser -[hidden]-> AddressBookParser
destroy AddApptCommandParser

AddressBookParser --> LogicManager : c
deactivate AddressBookParser

LogicManager -> AddApptCommand : execute(m)
activate AddApptCommand

AddApptCommand -> Model : fetchPersonIfPresent(S1234567A)
activate Model
Model -> AddApptCommand : target

create Appointment
AddApptCommand -> Appointment
deactivate Appointment

create Person
AddApptCommand -> Person : createEditedPerson(target, a)
AddApptCommand -> Model : setPerson(target, edited)
deactivate Model

create CommandResult
AddApptCommand -> CommandResult
activate CommandResult

CommandResult --> AddApptCommand
deactivate CommandResult

AddApptCommand --> LogicManager : r
deactivate AddApptCommand

[<--LogicManager
deactivate LogicManager
@enduml
